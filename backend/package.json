{
  "name": "microcare-backend",
  "version": "1.0.0",
  "description": "MicroCare mental health journaling backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:init": "ts-node scripts/init-db.ts",
